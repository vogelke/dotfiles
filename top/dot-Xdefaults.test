! -*- xrdb -*-

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!! XEMACS

!Emacs*background:       #c0c0c0
Emacs.default.attributeBackgroundPixmap: /usr/contrib/lib/X11/pixmaps/wh_marble_0.xpm

Emacs.default.attributeFont: -*-courier-medium-r-*-*-14-*-*-*-*-*-*-*
Emacs*menubar*font:           9x15bold
Emacs*menubar*fontList:           9x15bold

Emacs.geometry:		80x68+0+0
Emacs*EmacsFrame.geometry:	      80x68+0+0

Emacs.scrollBarWidth:  0
Emacs.scrollBarHeight:  0
Emacs.topToolBarHeight:               0
Emacs.bottomToolBarHeight:            0
Emacs.leftToolBarWidth:               0
Emacs.rightToolBarWidth:              0

*modelineShadowThickness:                2


!Emacs.modeline.attributeFont: 9x15

!Emacs*font:		-*-Clean-Medium-R-*-*-*-150-75-75-*-*-ISO8859-1

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!! MISC
! dla xfig
*customization: -color

!*visual: PseudoColor
!*visual: TrueColor

GVintern.printCommand: "lp -dnab319ps"

XDvi.allowshell: on
XDvi.background: white
XDvi.foreground: black
XDvi.expert: on
XDvi.magnifierSize1: 1280x1024
XDvi.geometry: 867x990+0+0


AcroRead.geometry: 900x990+0+0
Metafont*geometry: 300x450-0+0

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!! XTerm
XTerm*foreground:	black
XTerm*background:	gray80
XTerm*pointerColor:	red3
XTerm*cursorColor:	red3
XTerm*reverseWrap:	on

XTerm*font:		-misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-2
XTerm*boldFont:		-misc-fixed-bold-r-normal--15-140-75-75-c-90-iso8859-2


XTerm*saveLines:	5000
XTerm*jumpScroll:	on
XTerm*titleInhibit:	on
XTerm*multiScroll:	on
XTerm*scrollBar:	false
XTerm*scrollTtyOutput:	false
XTerm*scrollKey:	true

XTerm.borderWidth: 1
!*VT100.Translations: #override  \
!	 <Key>Home:       scroll-back(10,page)     \n\
!        <Key>End:        scroll-forw(10,page)     \n\
!        <Key>Prior:      scroll-back(1,page)     \n\
!        <Key>Next:       scroll-forw(1,page)
!*VT100.Translations: #override  \
!	<Key>Insert:       dabbrev-expand()


! for zsh (emacs style bindings)
XTerm*VT100.Translations: #override  \
                Ctrl<Key>Left:  string("\033b") \n\
                Ctrl<Key>Right: string("\033f") \n\
                <Key>Home:      string("\001") \n\
                <Key>End:       string("\005") \n\
                <Key>Delete:    string("\033[3~") \n\
                Shift<Key>Tab:  string("\033[Z") \n


!XTerm*UtmpInhibit:      True
XTerm*UtmpInhibit:      False
xterm*UtmpInhibit:      False

XTerm*VT100.geometry: 80x66+0+0

Rxvt.geometry: 80x66+0+0
Rxvt*colorBD:	red3

